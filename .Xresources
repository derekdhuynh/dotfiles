URxvt*transparent: true
URxvt*shading: 25

! Colorscheme

! special
*.foreground:   #70b4c9
*.background:   #1b2229
*.cursorColor:  #70b4c9

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #b11f1f
*.color9:       #cc5050

! green
*.color2:       #67b737
*.color10:      #67b737

! yellow
*.color3:       #E2C314
*.color11:      #E2C314

! blue
*.color4:       #5f819d
*.color12:      #81a2be

! magenta
*.color5:       #7f1f80
*.color13:      #aa68be

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #707880
*.color15:      #c5c8c6

URxvt.boldFont: xft:inconsolata:pixelsize=14
URxvt.font:xft:Inconsolata:regular:size=14

URxvt.perl-ext-common: selection-to-clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! Forces smooth scroll
! URxvt*jumpScroll: False
! URxvt*skipScroll: False
URxvt*scrollBar: False

!URxvt*borderLess: True
URxvt*cursorBlink: True
